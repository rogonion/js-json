{
  "name": "@rogonion/js-json",
  "private": false,
  "description": "A library for manipulating objects that resemble the structure and layout of JSON.",
  "version": "1.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "dist/types/index.d.ts",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "author": "Declan Joseph",
  "license": "MIT",
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc --project tsconfig.build.json && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "publish": "pnpm publish --access public"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rogonion/js-json.git"
  },
  "readme": "https://github.com/rogonion/js-json#README.md",
  "devDependencies": {
    "@types/node": "^24.5.2",
    "typescript": "~5.8.3",
    "vite": "^7.1.6",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "json",
    "objects",
    "manipulation",
    "utilities"
  ],
  "dependencies": {
    "yaml": "^2.8.1"
  }
}
